{
  
  "name": "Title Fetcher Extension",
  "description": "Fetches the title of the current tab and displays it.",
  "version":"1.0",
  "manifest_version":3,
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/text_icon16.png",
    "32": "/images/text_icon32.png",
    "48": "/images/text_icon48.png"
}
}